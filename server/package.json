{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "seed": "ts-node prisma/seed.ts",
    "seed:prod": "node dist/prisma/seed.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "prisma generate",
    "postprisma:generate": "shx cp -r node_modules/.prisma/client/index.d.ts ../client/src/types/prismaTypes.d.ts",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "echo \"WARNING: This will DELETE ALL DATA! Press Ctrl+C to cancel\" && timeout 5 && prisma migrate reset",
    "prisma:studio": "prisma studio",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,json,md}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@terraformer/wkt": "^2.2.1",
    "axios": "^1.13.5",
    "body-parser": "^2.2.2",
    "cloudinary": "^2.5.1",
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "formidable": "^3.5.4",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "prisma": "^6.19.0",
    "pusher": "^5.3.2",
    "stripe": "^20.3.1",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.3",
    "@types/cors": "^2.8.19",
    "@types/formidable": "^3.4.6",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.3.0",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.2",
    "@types/terraformer__wkt": "^2.0.3",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.27.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.14",
    "prettier": "^3.4.2",
    "rimraf": "^6.1.3",
    "shx": "^0.4.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
