# Mapbox Configuration
NEXT_PUBLIC_MAPBOX_ACCESS_TOKEN=your_mapbox_access_token

# AWS Cognito Configuration
NEXT_PUBLIC_AWS_COGNITO_USER_POOL_ID=your_cognito_user_pool_id
NEXT_PUBLIC_AWS_COGNITO_USER_POOL_CLIENT_ID=your_cognito_client_id

# API Configuration
NEXT_PUBLIC_API_BASE_URL=https://your-server-domain.vercel.app/api/v1

# Stripe Payment Configuration (publishable key only - safe for client)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key

# Pusher Configuration (for real-time messaging and notifications)
# Get these from Pusher Dashboard > Your App > App Keys
NEXT_PUBLIC_PUSHER_KEY=your_pusher_key
NEXT_PUBLIC_PUSHER_CLUSTER=your_pusher_cluster